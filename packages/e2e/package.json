{
  "private": true,
  "name": "e2e",
  "version": "1.0.0",
  "scripts": {
    "build": "rimraf build && mkdir build && cp -R node_modules build && cp -R tests build && cp runner.js build",
    "deploy": "dotenv -- cdk deploy --require-approval never --app infrastructure/app.js",
    "test": "dotenv jest --runInBand"
  },
  "jest": {
    "verbose": true,
    "preset": "jest-puppeteer",
    "setupFilesAfterEnv": []
  },
  "dependencies": {
    "@aws-cdk/aws-lambda": "^1.1.0",
    "@aws-cdk/core": "^1.1.0",
    "aws-cdk": "^1.1.0",
    "dotenv-cli": "^2.0.1",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^1.19.0",
    "rimraf": "^2.6.3"
  }
}
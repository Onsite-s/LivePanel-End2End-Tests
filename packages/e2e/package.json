{
  "private": true,
  "name": "e2e",
  "version": "1.0.0",
  "scripts": {
    "build": "rimraf build && mkdir build && cp -R package.json build && npm install --production --prefix build && cp -R tests build && cp -R jest-puppeteer.config.js build && cp runner.js build",
    "deploy": "dotenv -- cdk deploy --require-approval never --app infrastructure/app.js",
    "test": "dotenv jest --runInBand"
  },
  "dependencies": {
    "chrome-aws-lambda": "^1.19.0",
    "chrome-paths": "^1.0.0",
    "jest": "^24.8.0",
    "jest-environment-puppeteer": "^4.3.0",
    "jest-puppeteer": "^4.3.0",
    "module-alias": "^2.1.0",
    "puppeteer-core": "^1.19.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-lambda": "^1.1.0",
    "@aws-cdk/core": "^1.1.0",
    "aws-cdk": "^1.1.0",
    "aws-sdk": "^2.498.0",
    "dotenv-cli": "^2.0.1",
    "rimraf": "^2.6.3"
  },
  "_moduleAliases": {
    "puppeteer": "node_modules/puppeteer-core"
  }
}
